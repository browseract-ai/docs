  "components": {
    "schemas": {
      "ResponseData": {
        "type": "object",
        "description": "Unified response entity",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32",
            "description": "Response code, 0 means success, \u003E0 means failure"
          },
          "msg": {
            "type": "string",
            "description": "Response message, returns error reference information when code\u003E0"
          },
          "data": {
            "type": "object",
            "description": "Response data"
          },
          "ts": {
            "type": "integer",
            "format": "int64",
            "description": "Server timestamp"
          },
          "time": {
            "type": "string",
            "format": "date-time"
          },
          "traceId": {
            "type": "string",
            "description": "Error ID for error tracking in logs"
          }
        }
      },
      "InputParamItemDto": {
        "type": "object",
        "description": "Input Param Item",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name"
          },
          "value": {
            "type": "string",
            "description": "Value"
          }
        }
      },
      "RunWorkflowTaskRequest": {
        "type": "object",
        "description": "Request object to initiate a new task execution.",
        "properties": {
          "workflow_id": {
            "type": "string",
            "description": "Which workflow you need to run.",
            "title": "Workflow-id"
          },
          "input_parameters": {
            "type": "array",
            "description": "Input parameters for the workflow",
            "items": {
              "$ref": "#/components/schemas/InputParamItemDto"
            },
            "title": "Input Parameters"
          },
          "save_browser_data": {
            "type": "boolean",
            "description": "If set to true, a profile_id will be returned upon successful task initiation. This ID can be used to reuse browser data generated during this task.",
            "title": "Save Browser Data"
          },
          "profile_id": {
            "type": "string",
            "description": "Use a previously returned profile_id to reuse browser data (e.g., cookies) from an earlier task.",
            "title": "Browser Profile Id"
          },
          "open_incognito_mode": {
            "type": "boolean",
            "deprecated": true,
            "title": "Open Incognito Mode"
          },
          "callback_url": {
            "type": "string",
            "description": "Optional. HTTP/HTTPS URL to receive task completion notifications. The system will send a POST request with task results when the task finishes, fails, or is canceled. Maximum length: 2048 characters. Cannot point to private/local addresses.",
            "example": "https://your-server.com/webhooks/workflow-callback",
            "maxLength": 2048,
            "minLength": 0,
            "title": "Callback URL"
          },
          "status_change_callback_url": {
            "type": "string",
            "description": "Optional. HTTP/HTTPS URL to receive task status change notifications. The system will send a POST request with task results when the task's status change, such as running, finished, canceled, paused, failed. Maximum length: 2048 characters. Cannot point to private/local addresses.",
            "example": "https://your-server.com/webhooks/task-status-change-callback",
            "maxLength": 2048,
            "minLength": 0,
            "title": "Status Change Callback URL"
          }
        }
      },
      "TaskCreatedResponse": {
        "type": "object",
        "description": "Response containing details of the created task.",
        "properties": {
          "id": {
            "type": "string",
            "description": "The unique task_id for this task, which can be used to track its progress or result."
          },
          "profile_id": {
            "type": "string",
            "description": "The profile_id will only be returned if save_browser_data is set to true, enabling reuse of browser data generated during this task."
          }
        },
        "required": [
          "id"
        ]
      },
      "RunWorkflowTemplateTaskRequest": {
        "type": "object",
        "description": "Request object to initiate base template a new  task execution.",
        "properties": {
          "proxyRegion": {
            "type": "string",
            "description": "Optional. Region where the proxy should be used.  default US",
            "example": "US",
            "title": "Proxy Region"
          },
          "workflow_template_id": {
            "type": "string",
            "description": "Which workflow you need to run.",
            "title": "Workflow-template-id"
          },
          "input_parameters": {
            "type": "array",
            "description": "Input parameters for the workflow",
            "items": {
              "$ref": "#/components/schemas/InputParamItemDto"
            },
            "title": "Input Parameters"
          },
          "callback_url": {
            "type": "string",
            "description": "Optional. HTTP/HTTPS URL to receive task completion notifications. The system will send a POST request with task results when the task finishes, fails, or is canceled. Maximum length: 2048 characters. Cannot point to private/local addresses.",
            "example": "https://your-server.com/webhooks/workflow-callback",
            "maxLength": 2048,
            "minLength": 0,
            "title": "Callback URL"
          },
          "status_change_callback_url": {
            "type": "string",
            "description": "Optional. HTTP/HTTPS URL to receive task status change notifications. The system will send a POST request with task results when the task's status change, such as running, finished, canceled, paused, failed. Maximum length: 2048 characters. Cannot point to private/local addresses.",
            "example": "https://your-server.com/webhooks/task-status-change-callback",
            "maxLength": 2048,
            "minLength": 0,
            "title": "Status Change Callback URL"
          }
        }
      },
      "ApiWorkflowDto": {
        "type": "object",
        "description": "Workflow Info",
        "properties": {
          "id": {
            "type": "string",
            "description": "Workflow ID"
          },
          "name": {
            "type": "string",
            "description": "Workflow's name"
          },
          "description": {
            "type": "string",
            "description": "Workflow's description"
          },
          "create_at": {
            "type": "string",
            "format": "date-time",
            "description": "Workflow created time"
          },
          "publish_at": {
            "type": "string",
            "format": "date-time",
            "description": "Workflow Publish Time"
          }
        }
      },
      "PageRespApiWorkflowDto": {
        "type": "object",
        "description": "Paginated response containing items and metadata.",
        "properties": {
          "page": {
            "type": "integer",
            "format": "int32",
            "description": "Current page number."
          },
          "limit": {
            "type": "integer",
            "format": "int32",
            "description": "Number of items per page."
          },
          "items": {
            "type": "array",
            "description": "List of items for the current page.",
            "items": {
              "$ref": "#/components/schemas/ApiWorkflowDto"
            }
          },
          "total_pages": {
            "type": "integer",
            "format": "int32",
            "description": "Total number of pages available."
          },
          "total_count": {
            "type": "integer",
            "format": "int32",
            "description": "Number of total items."
          }
        }
      },
