    "/v2/workflow/list-tasks": {
      "get": {
        "tags": [
          "api-v-2-workflow-controller"
        ],
        "summary": "Get a list of tasks",
        "description": "Retrieve paginated list of tasks with optional status filtering.",
        "operationId": "pageTasks",
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "description": "Filter tasks by status. Valid values: created, running, finished, stopped, paused, failed",
            "required": false,
            "schema": {
              "type": "string",
              "description": "Enumeration of possible task states.\n\n- created: Task is initialized but not yet started\n- running: Task is currently executing\n- finished: Task has completed successfully\n- stopped: Task was manually stopped\n- paused: Task execution is temporarily paused\n- failed: Task encountered an error and could not complete",
              "enum": [
                "created",
                "running",
                "finished",
                "canceled",
                "pausing",
                "paused",
                "failed",
                "unknown"
              ]
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Page number to retrieve. Minimum value is 1.",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 1
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Number of tasks per page. Maximum value is 500.",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 500,
              "minimum": 1
            }
          },
          {
            "name": "workflow_id",
            "in": "query",
            "description": "Workflow Id",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "400": {
            "description": "Bad Request",
            "content": {
              "*/*": {
                "schema": {
